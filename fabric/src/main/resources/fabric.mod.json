{
  "schemaVersion": 1,
  "id": "hex_expanded",
  "version": "${version}",
  "name": "Hex expanded",
  "description": "Hex expanded addon for Hex Casting",
  "authors": [
    "NathanSnail"
  ],
  "contact": {
    "homepage": "https://nathansnail.github.io/hex_expanded",
    "sources": "https://github.com/NathanSnail/hex_expanded"
  },
  "license": "MIT",
  "icon": "assets/hex_expanded/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "io.github.nathansnail.hex_expanded.fabric.FabricHex_expanded"
    }],
    "client": [{
      "adapter": "kotlin",
      "value": "io.github.nathansnail.hex_expanded.fabric.FabricHex_expandedClient"
    }],
    "server": [{
      "adapter": "kotlin",
      "value": "io.github.nathansnail.hex_expanded.fabric.FabricHex_expandedServer"
    }],
    "fabric-datagen": [{
      "adapter": "kotlin",
      "value": "io.github.nathansnail.hex_expanded.fabric.datagen.FabricHex_expandedDatagen"
    }],
    "modmenu": [{
      "adapter": "kotlin",
      "value": "io.github.nathansnail.hex_expanded.fabric.FabricHex_expandedModMenu"
    }]
  },
  "mixins": [
    "hex_expanded-common.mixins.json",
    "hex_expanded-fabric.mixins.json"
  ],
  "depends": {
    "minecraft": "${versions.minecraft}",
    "fabricloader": ">=${versions.fabric_loader}",
    "fabric-api": ">=${versions.fabric_api}",
    "fabric-language-kotlin": ">=${versions.kotlin_fabric}",
    "architectury": ">=${versions.architectury}",
    "hexcasting": ">=${versions.hexcasting_dependency}",
    "cloth-config": ">=${versions.clothConfig}"
  },
  "suggests": {
    "modmenu": ">=${versions.modMenu}"
  }
}
